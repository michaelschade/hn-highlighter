{
  "name": "Hacker News Highlighter",
  "version": "0.2.2",
  "manifest_version": 2,

  "description": "Improve Hacker News by highlighting new or updated posts.",
  "icons": {
    "16":  "icon16.png",
    "48":  "icon48.png",
    "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://news.ycombinator.com/*"],
      "js": ["hn-highlighter.user.js"]
    }
  ]
}
